{
  "name": "cv-maker",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "start": "nx serve",
    "dev": "nx run-many --target=serve --projects=frontend,api --parallel=2",
    "build": "nx build",
    "test": "nx test",
    "db:create": "ts-node node_modules/.bin/sequelize db:create",
    "db:migrate": "ts-node node_modules/.bin/sequelize db:migrate",
    "db:seed": "ts-node node_modules/.bin/sequelize db:seed:all",
    "db:drop": "ts-node node_modules/.bin/sequelize db:drop",
    "db:init": "npm run db:create && npm run db:migrate && npm run db:seed",
    "db:reset": "npm run db:drop && npm run db:init"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.0.0",
    "@angular/common": "^13.0.0",
    "@angular/compiler": "^13.0.0",
    "@angular/core": "^13.0.0",
    "@angular/forms": "^13.0.0",
    "@angular/platform-browser": "^13.0.0",
    "@angular/platform-browser-dynamic": "^13.0.0",
    "@angular/router": "^13.0.0",
    "@auth0/angular-jwt": "^5.0.2",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/sequelize": "^8.0.0",
    "@nrwl/angular": "13.3.2",
    "dotenv": "^10.0.0",
    "mysql2": "^2.3.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~7.4.0",
    "sequelize": "^6.11.0",
    "sequelize-cli": "^6.3.0",
    "sequelize-typescript": "^2.1.1",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.0.0",
    "@angular-eslint/eslint-plugin": "~13.0.1",
    "@angular-eslint/eslint-plugin-template": "~13.0.1",
    "@angular-eslint/template-parser": "~13.0.1",
