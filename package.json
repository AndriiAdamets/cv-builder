    "@angular/compiler-cli": "^13.0.0",
    "@angular/language-service": "^13.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@nrwl/cli": "13.3.2",
    "@nrwl/cypress": "13.3.2",
    "@nrwl/eslint-plugin-nx": "13.3.2",
    "@nrwl/jest": "13.3.2",
    "@nrwl/linter": "13.3.2",
    "@nrwl/nest": "13.3.2",
    "@nrwl/node": "13.3.2",
    "@nrwl/tao": "13.3.2",
    "@nrwl/workspace": "13.3.2",
    "@types/jest": "27.0.2",
    "@types/node": "14.14.33",
